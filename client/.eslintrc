{
  extends: ["airbnb", "plugin:prettier/recommended"],
  plugins: [ "prettier"],
  parser: "babel-eslint",
  parserOptions: {
    ecmaVersion: 2016,
    sourceType: "module"
  },
  env: {
    es6: true,
    browser: true,
    node: true
  },
  rules: {
    "prettier/prettier": "error",
    react/jsx-filename-extension: [ 1,
      {
        extensions: [
          ".js",
          ".jsx"
        ]
      }
    ],
    no-underscore-dangle: [ 1,
      {
        allow: [
          "_id"
        ]
      }
    ]
  }
}